{
  "author": "fusionxo",
  "homepage_url": "https://github.com/fusionxo/Simple-Media-Controller",
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": "script-src 'self' https://cdn.tailwindcss.com; object-src 'self'",
  "icons": {
    "256": "sound-recognition.png"
  },
  "manifest_version": 2,
  "name": "Media Controller",
  "permissions": ["tabs"],
  "version": "1.0.1",
  "web_accessible_resources": ["attach.js"],
  "browser_specific_settings": {
    "gecko": {
      "id": "kami@calverse.in",
      "strict_min_version": "109.0"
    }
  }
}
